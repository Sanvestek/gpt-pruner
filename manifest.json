{
  "manifest_version": 3,
  "name": "ChatGPT Message History Pruner",
  "version": "1.0",
  "icons": {
    "128": "icon.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "permissions": ["activeTab", "scripting"],
  "host_permissions": ["https://chatgpt.com/*"]
}